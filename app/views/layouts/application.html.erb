<!DOCTYPE html>
<html>
<head>
  <title>Flapper</title>
  <%= stylesheet_link_tag    'application', media: 'all' %>
  <%= javascript_include_tag 'application' %>
  <%= csrf_meta_tags %>
</head>
<body>

  <div ng-app="myApp">
    <div class="container">
      <div ng-include="'auth/_navbar.html'"></div>
